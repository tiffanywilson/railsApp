<h1>Welcome to Star Battle</h1>
<p>Where the Force decides your fate.</p>
<div class="container">
  <% @feedback.each do |f| %>
  <div class="media">
    <div class="media-left">
      <a href="#">
        <img class="media-object" src="http://www.gravatar.com/avatar/424242"
        >
        </a>
        </div>
        <div class="media-body">
          <h4 class-"media-heading"><%= f.name %></h4>
          <div><%= time_ago_in_words f.created_at  %> ago</div>
          <%= f.response %>
        </div>
  </div>
  <% end %>
</div>
